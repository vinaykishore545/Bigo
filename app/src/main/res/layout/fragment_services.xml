<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="horizontal"
        android:layout_weight="0.7"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:baselineAligned="false">
        <LinearLayout
            android:layout_weight="0.5"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            tools:ignore="NestedWeights">
            <TextView
                android:id="@+id/personal"
                android:background="@color/colorPrimary"
                android:gravity="center"
                android:text="@string/personal_use"
                android:textSize="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:ignore="HardcodedText" />


        </LinearLayout>
        <LinearLayout
            android:layout_weight="0.5"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/bussiness"
                android:gravity="center"
                android:text="@string/for_Bussiness"
                android:textSize="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:ignore="HardcodedText" />


        </LinearLayout>



    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_weight="9"
        android:layout_width="match_parent"
        android:layout_height="0dp">
        <LinearLayout
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10ssp"
            android:layout_marginEnd="@dimen/_10ssp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120sdp"
            android:baselineAligned="false">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_width="match_parent"
                app:cardUseCompatPadding="true"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false">

                    <LinearLayout
                        android:paddingStart="@dimen/_10sdp"

                        android:orientation="vertical"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        tools:ignore="RtlSymmetry">
                        <TextView
                            android:layout_marginTop="@dimen/_10sdp"
                            android:textSize="@dimen/_15sdp"
                            android:textColor="#000"
                            android:text="@string/car_pick_up"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp">

                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/ic_nigeria1"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                tools:ignore="ContentDescription" />
                            <LinearLayout
                                android:gravity="center"
                                android:layout_gravity="center"
                                android:layout_weight="0.2"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_gravity="center"
                                    android:textSize="@dimen/_20sdp"
                                    android:textColor="#fbac3a"
                                    android:text="@string/number"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    tools:ignore="HardcodedText" />

                            </LinearLayout>
                            <LinearLayout

                                android:layout_weight="0.6"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:layout_gravity="center"
                                    android:text="@string/per_hour"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    tools:ignore="HardcodedText" />

                            </LinearLayout>





                        </LinearLayout>
                        <TextView
                            android:text="@string/view_more"
                            android:textSize="@dimen/_12sdp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />








                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">
                        <androidx.cardview.widget.CardView
                            android:layout_marginStart="@dimen/_15sdp"
                            app:cardCornerRadius="@dimen/_360sdp"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp">
                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/ic_ordercar"
                                android:layout_width="@dimen/_25sdp"
                                android:layout_height="@dimen/_25sdp"
                                tools:ignore="ContentDescription" />

                        </androidx.cardview.widget.CardView>
                        <Button
                            android:layout_marginTop="@dimen/_4sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginEnd="@dimen/_2sdp"
                            android:background="@drawable/round_button"
                            android:textColor="#fff"
                            android:text="@string/select"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_25sdp"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10ssp"
            android:layout_marginEnd="@dimen/_10ssp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120sdp"
            android:baselineAligned="false">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_width="match_parent"
                app:cardUseCompatPadding="true"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false">

                    <LinearLayout
                        android:paddingStart="@dimen/_10sdp"

                        android:orientation="vertical"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        tools:ignore="RtlSymmetry">
                        <TextView
                            android:layout_marginTop="@dimen/_10sdp"
                            android:textSize="@dimen/_15sdp"
                            android:textColor="#000"
                            android:text="@string/personal_hourly"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp">

                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/ic_nigeria1"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                tools:ignore="ContentDescription" />
                            <LinearLayout
                                android:gravity="center"
                                android:layout_gravity="center"
                                android:layout_weight="0.2"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_gravity="center"
                                    android:textSize="@dimen/_20sdp"
                                    android:textColor="#fbac3a"
                                    android:text="@string/number"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    tools:ignore="HardcodedText" />

                            </LinearLayout>
                            <LinearLayout

                                android:layout_weight="0.6"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:layout_gravity="center"
                                    android:text="@string/per_hour"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    tools:ignore="HardcodedText" />

                            </LinearLayout>





                        </LinearLayout>
                        <TextView
                            android:text="@string/view_more"
                            android:textSize="@dimen/_12sdp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />








                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">
                        <androidx.cardview.widget.CardView
                            android:layout_marginStart="@dimen/_15sdp"
                            app:cardCornerRadius="@dimen/_360sdp"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp">
                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/ic_driving"
                                android:layout_width="@dimen/_25sdp"
                                android:layout_height="@dimen/_25sdp"
                                tools:ignore="ContentDescription" />

                        </androidx.cardview.widget.CardView>
                        <Button
                            android:layout_marginTop="@dimen/_4sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginEnd="@dimen/_2sdp"
                            android:background="@drawable/round_button"
                            android:textColor="#fff"
                            android:text="@string/select"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_25sdp"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10ssp"
            android:layout_marginEnd="@dimen/_10ssp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120sdp"
            android:baselineAligned="false">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_width="match_parent"
                app:cardUseCompatPadding="true"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false">

                    <LinearLayout
                        android:paddingStart="@dimen/_10sdp"

                        android:orientation="vertical"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        tools:ignore="RtlSymmetry">
                        <TextView
                            android:layout_marginTop="@dimen/_10sdp"
                            android:textSize="@dimen/_15sdp"
                            android:textColor="#000"
                            android:text="@string/driver_team_event"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp">

                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/ic_nigeria1"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                tools:ignore="ContentDescription" />
                            <LinearLayout
                                android:gravity="center"
                                android:layout_gravity="center"
                                android:layout_weight="0.2"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_gravity="center"
                                    android:textSize="@dimen/_20sdp"
                                    android:textColor="#fbac3a"
                                    android:text="@string/number"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>

                            </LinearLayout>
                            <LinearLayout

                                android:layout_weight="0.6"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:layout_gravity="center"
                                    android:text="@string/per_hour"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>

                            </LinearLayout>





                        </LinearLayout>
                        <TextView
                            android:text="@string/view_more"
                            android:textSize="@dimen/_12sdp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>








                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">
                        <androidx.cardview.widget.CardView
                            android:layout_marginStart="@dimen/_15sdp"
                            app:cardCornerRadius="@dimen/_360sdp"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp">
                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/ic_location"
                                android:layout_width="@dimen/_25sdp"
                                android:layout_height="@dimen/_25sdp"
                                tools:ignore="ContentDescription" />

                        </androidx.cardview.widget.CardView>
                        <Button
                            android:layout_marginTop="@dimen/_4sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginEnd="@dimen/_2sdp"
                            android:background="@drawable/round_button"
                            android:textColor="#fff"
                            android:text="@string/select"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_25sdp"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


        </LinearLayout>



    </LinearLayout>


</LinearLayout>