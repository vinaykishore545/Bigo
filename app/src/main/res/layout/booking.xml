<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_weight="9"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        tools:ignore="UselessParent">
        <LinearLayout
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginStart="@dimen/_10ssp"
            android:layout_marginEnd="@dimen/_10ssp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_200sdp"
            android:baselineAligned="false"
            tools:ignore="UselessParent">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="@dimen/_5sdp"
                android:outlineSpotShadowColor="@color/colorPrimary"
                android:layout_width="match_parent"
                app:cardUseCompatPadding="true"
                android:layout_height="match_parent"
                tools:ignore="UnusedAttribute">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false">

                    <LinearLayout
                        android:paddingStart="@dimen/_10sdp"

                        android:orientation="vertical"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        tools:ignore="RtlSymmetry">
                        <TextView
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:textSize="@dimen/_15sdp"
                            android:text="Date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:textColor="#000"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:id="@+id/date_booking"
                            android:text="23/03/2020"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:textSize="@dimen/_15sdp"
                            android:text="Time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:textColor="#000"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:id="@+id/time_booking"
                            android:text="23:30Am"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:textSize="@dimen/_15sdp"
                            android:text="Service ID"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:textColor="#000"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:id="@+id/booking_serid"
                            android:text="23:30Am"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="right"
                        android:orientation="vertical"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_marginEnd="@dimen/_15sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:textSize="@dimen/_15sdp"
                            android:text="@string/pick_in"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_marginEnd="@dimen/_15sdp"
                            android:textColor="#000"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:id="@+id/pickin_booking"
                            android:text="23/03/2020"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:layout_marginEnd="@dimen/_15sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:textSize="@dimen/_15sdp"
                            android:text="pick_up"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:layout_marginEnd="@dimen/_15sdp"
                            android:textColor="#000"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:id="@+id/pickup_booking"
                            android:text="23/03/2020"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:layout_marginEnd="@dimen/_15sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:textSize="@dimen/_15sdp"
                            android:text="price"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                        <TextView
                            android:layout_marginEnd="@dimen/_15sdp"
                            android:textColor="#000"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:id="@+id/booking_price"
                            android:text="23/03/2020"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="HardcodedText" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>